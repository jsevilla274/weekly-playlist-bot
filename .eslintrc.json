{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "ignorePatterns": ["**/*.json"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-prototype-builtins": "off",
        "spaced-comment": "off",
        "semi": ["warn", "always", { "omitLastInOneLineBlock": true }],
        "no-inner-declarations": "off",
        "no-redeclare":"off",
        "no-unsafe-finally": "off",
        "no-constant-condition":"warn"
    }
}
